{
  "envId": "{{env.ENV_ID}}",
  "version": "2.0",
  "$schema": "https://framework-1258016615.tcloudbaseapp.com/schema/latest.json",
  "framework": {
    "name": "cea-campus-sign",
    "plugins": {
      "func": {
        "use": "@cloudbase/framework-plugin-function",
        "inputs": {
          "functionRootPath": "./serverless/",
          "functions": [
            {
              "name": "cea-tcb",
              "memory": 512,
              "timeout": 20,
              "runtime": "Nodejs12.16",
              "installDependency": true,
              "handler": "index.main",
              "triggers": [
                {
                  "name": "sign",
                  "type": "timer",
                  "config": "0 0 6 * * * *"
                },
                {
                  "name": "attendance",
                  "type": "timer",
                  "config": "0 0 21 * * * *"
                }
              ]
            }
          ]
        }
      }
    }
  }
}
