  "version": "0.2.0",
  "configurations": [
    {
      "type": "pwa-node",
      "request": "launch",
      "name": "Sign Check In Test",
      "skipFiles": ["**/node_modules/**"],
      "preLaunchTask": "npm: build:debug",
      "program": "${workspaceFolder}/src/internal/src/cli.ts",
      "args": ["sign"],
      "outFiles": ["${workspaceFolder}/src/**/*.js"]
    },
    {
      "type": "pwa-node",
      "request": "launch",
      "name": "Attendance Check In Test",
      "skipFiles": ["**/node_modules/**"],
      "preLaunchTask": "npm: build:debug",
      "program": "${workspaceFolder}/src/internal/src/cli.ts",
      "args": ["attendance"],
      "outFiles": ["${workspaceFolder}/src/**/*.js"]
    },
    {
      "type": "pwa-node",
      "request": "launch",
      "name": "Load Config Test",
      "skipFiles": ["**/node_modules/**"],
      "preLaunchTask": "npm: build:debug",
      "program": "${workspaceFolder}/src/internal/src/cli.ts",
      "args": ["load"],
      "outFiles": ["${workspaceFolder}/src/**/*.js"]
    }
  ]
}
