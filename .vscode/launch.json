  "version": "0.2.0",
  "configurations": [
    {
      "type": "pwa-node",
      "request": "launch",
      "name": "Check In Test",
      "skipFiles": ["<node_internals>/**"],
      "preLaunchTask": "npm: build:debug",
      "program": "${workspaceFolder}/internal/src/cli.ts",
      "args": ["sign"],
      "outFiles": ["${workspaceFolder}/**/*.js"]
    },
    {
      "type": "pwa-node",
      "request": "launch",
      "name": "Load Config Test",
      "skipFiles": ["<node_internals>/**"],
      "preLaunchTask": "npm: build:debug",
      "program": "${workspaceFolder}/internal/src/cli.ts",
      "args": ["load"],
      "outFiles": ["${workspaceFolder}/**/*.js"]
    }
  ]
}
